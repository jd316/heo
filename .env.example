# Environment variables for HEO ElizaOS Plugin example
# Copy this file to .env and fill in values

# Environment Mode and Server
NODE_ENV=development
PORT=3000
HOST=localhost

# Authentication (JWT)
JWT_SECRET=<your-32+ character secret>
JWT_EXPIRY=86400

# Google Gemini (Hypothesis Generation)
GEMINI_API_KEY=<your Gemini API key>
GEMINI_MODEL_NAME=gemini-1.5-flash-latest

# Expose Gemini keys to browser
NEXT_PUBLIC_GEMINI_API_KEY=<your Gemini API key>
NEXT_PUBLIC_GEMINI_MODEL_NAME=gemini-1.5-flash-latest

# Knowledge Graph (OxiGraph / OriginTrail DKG)
OXIGRAPH_ENDPOINT_URL=http://localhost:7878
ORIGINTRAIL_NODE_HOSTNAME=http://localhost
ORIGINTRAIL_NODE_PORT=7878
OT_NODE_HOSTNAME=http://127.0.0.1
OT_NODE_PORT=8900

# DKG Service (dkg.js)
BLOCKCHAIN_ID=hardhat1:31337
ENVIRONMENT=development
PUBLIC_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80
PRIVATE_KEY=0xYourPrivateKeyHere

# Cloud-Lab Execution (Strateos/ECL)
ECL_API_URL=https://app.strateos.com/api
ECL_API_KEY=<your ECL API key>

# Solana Blockchain Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_KEYPAIR_PATH=./keypair.json
SOLANA_CLUSTER=devnet

# IPFS Storage (Pinata or local node)
IPFS_ENDPOINT=http://localhost:5001/api/v0
IPFS_GATEWAY_URL=https://ipfs.io/ipfs/
IPFS_API_KEY=<your Pinata API key>
IPFS_API_SECRET=<your Pinata API secret>
PINATA_JWT=<your Pinata JWT>
IS_PRODUCTION=false

# zkSNARK Circuit Paths
PROTOCOL_CHECK_CIRCUIT_WASM_PATH=circuits/protocol_check_js/protocol_check.wasm
PROTOCOL_CHECK_CIRCUIT_ZKEY_PATH=circuits/protocol_check_final.zkey
PROTOCOL_CHECK_VERIFICATION_KEY_PATH=circuits/verification_key.json

# Logging and Feature Flags
LOG_LEVEL=info
ENABLE_HYPOTHESIS_GENERATION=true

# Database (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/yourdb

# Optional Performance Tuning
# MAX_CONCURRENT_REQUESTS=50
# CACHE_TTL=3600